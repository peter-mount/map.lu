  <Layer name="roads" status="on" srs="&osm2pgsql_projection;">
    <StyleName>minor-roads-fill</StyleName>
    <StyleName>roads</StyleName>
    <Datasource>
      <Parameter name="table">
      (select way,highway,
       case when tunnel in ('yes','true','1') then 'yes'::text else tunnel end as tunnel,
       case when railway='preserved' and service in ('spur','siding','yard') then 'INT-preserved-ssy'::text else railway end as railway
       from &prefix;_roads
       where railway is not null and railway!='preserved'
	  and (service is null or service not in ('spur','siding','yard'))
       order by z_order
      ) as roads
      </Parameter>
      &datasource-settings;
    </Datasource>
  </Layer>

</Map>
