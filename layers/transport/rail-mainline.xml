      &minscale_zoom10;
      <LineSymbolizer>
      <CssParameter name="stroke">#809bc0</CssParameter>
      <CssParameter name="stroke-width">2</CssParameter>
    </LineSymbolizer>
   </Rule>
   <Rule>
     <Filter>[highway] = 'motorway' or [highway] = 'motorway_link'</Filter>
